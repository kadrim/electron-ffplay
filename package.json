{
  "name": "electron-ffplay",
  "version": "1.0.1",
  "author": "R.J.",
  "description": "An electron-vue project",
  "license": "MIT",
  "main": "dist/electron/main.js",
  "scripts": {
    "build:pack": "electron-builder",
    "build:dir": "electron-builder --dir",
    "build:dist": "node --max_old_space_size=32768 .electron-vue/build.js",
    "build": "node --max_old_space_size=32768 .electron-vue/build.js && electron-builder",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "configure:ffplay": "node cmake-tool configure -d node-ffplay --CDTHIRD_PARTY_DIR=../.third-party",
    "build:ffplay": "node cmake-tool build -d node-ffplay",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js"
  },
  "build": {
    "asar": true,
    "productName": "electron-ffplay",
    "appId": "com.steveista.app",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      "licenses/**/*",
      "*.dll"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "resources/icons/icon.icns"
    },
    "win": {
      "icon": "resources/icons/icon.ico"
    },
    "linux": {
      "icon": "resources/icons"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.5.5",
    "@mdi/font": "^4.1.95",
    "ajv": "^6.5.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "balloon-css": "^1.0.4",
    "big-integer": "^1.6.48",
    "bluebird": "^3.7.2",
    "brace": "^0.11.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^8.2.2",
    "electron-builder": "^20.43.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^4.0.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "moment": "^2.24.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "npmlog": "^4.1.2",
    "raw-loader": "^4.0.0",
    "rc": "^1.2.8",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "selectn": "^1.1.2",
    "spectron": "^3.8.0",
    "splitargs": "0.0.7",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue": "^2.6.10",
    "vue-devtools": "^5.1.3",
    "vue-html-loader": "^1.2.4",
    "vue-i18n": "^8.11.2",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuetify": "^2.1.15",
    "vuex": "^3.1.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3",
    "which": "^2.0.2",
    "xml2js": "^0.4.22",
    "yml-loader": "^2.1.0"
  }
}
